---
import MainLayout from "../layouts/MainLayout.astro";
import CTA from "@components/CTA.astro";
import PageTitle from "@components/Page-Title.astro";
import TopCanadaCourses from "@components/TopCanadaCourses.astro";
import WhyChooseCanada from "@components/WhyChooseCanada.astro";
import TopCountries from "../components/TopCountries.astro";
import CanadaContent1 from "@components/Canada-content1.astro";
import OurPartners from "@components/OurPartners.astro";
import FAQ from "@components/FAQ.astro";
import Testimonials from "@components/Testimonials.astro";
import Contact from "@components/Contact.astro";
import LifeInAustralia from "@components/Life_in_Australia.astro";
import TabComponent from "@components/TabComponent.astro";
// images
import pageBannerImg from "@assets/img/country-img/canada-banner.jpg";

const pageTitle = "A One Global";
const pageDescription = "";
const pageKeywords = "";
const pageAuthor = "";

const pageBanner = { url: pageBannerImg.src };
---

<!-- src/pages/index.astro -->
<MainLayout title={pageTitle} description={pageDescription} keywords={pageKeywords} author={pageAuthor}>
  <main>
    <PageTitle title="Study in Canada" banner={pageBanner} />
    <CanadaContent1 />
    <div class="section-divider"></div>
    <CTA />
    <TopCanadaCourses />
    <WhyChooseCanada />
    <LifeInAustralia />
    <TabComponent />
    <FAQ />
    <Testimonials />
    <TopCountries />
    <OurPartners />
    <Contact />
  </main>
</MainLayout>
